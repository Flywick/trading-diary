{
  "expo": {
    "name": "trading-diary",
    "slug": "trading-diary",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-trading-diary.png",
    "scheme": "tradingdiary",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.anonymous.tradingdiary",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon2.png",
        "backgroundColor": "#020617"
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "splash": {
      "image": "./assets/images/icon-splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B1220",
      "dark": {
        "image": "./assets/images/icon-splash-screen-dark.png",
        "backgroundColor": "#020617"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon-splash-screen.png",
          "imageWidth": 240,
          "resizeMode": "contain",
          "backgroundColor": "#0B1220",
          "dark": {
            "image": "./assets/images/icon-splash-screen-dark.png",
            "backgroundColor": "#020617"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
